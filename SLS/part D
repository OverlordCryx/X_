local grass = workspace.Stadium.Field.Grass
local partName = "NOTHING X"
if not workspace:FindFirstChild(partName) then
    local part = Instance.new("Part")
    part.Anchored = true
    part.Size = Vector3.new(2048, 50, 2048)
    part.BrickColor = BrickColor.new("Really black")
    part.Material = Enum.Material.ForceField
    part.Transparency = 0.9
    part.CanCollide = true
    part.Name = partName
    part.Position = grass.Position - Vector3.new(0, 25.1, 0)
    part.Parent = workspace
else
end
local LB = workspace.Lobby.Build.Floor.SpawnCircle.LogoFIFA
local partName = "NOTHING XX"
if not workspace:FindFirstChild(partName) then
    local part = Instance.new("Part")
    part.Anchored = true
    part.Size = Vector3.new(130, 60, 250)
    part.BrickColor = BrickColor.new("Really black")
    part.Material = Enum.Material.ForceField
    part.Transparency = 0.93
    part.CanCollide = false
    part.Name = partName
    part.Position = LB.Position + Vector3.new(0, 15, 0)
    part.Parent = workspace
else
end
