local Players = game:GetService("Players")
local Lighting = game:GetService("Lighting")
local Workspace = game:GetService("Workspace")
local player = Players.LocalPlayer
local gui = player:WaitForChild("PlayerGui")
local gameGui = gui:WaitForChild("GameGui")
local lobby = Workspace:WaitForChild("Lobby")
local miscFolder = Workspace:WaitForChild("Misc")
local function clearFolder(folder)
    for _, obj in pairs(folder:GetChildren()) do
        obj:Destroy()
    end
end
local contentFolder = lobby:WaitForChild("Content")
clearFolder(contentFolder)
local buildFolder = lobby:WaitForChild("Build")
local foldersToDelete = {
    "Plants probably temp",
    "Model",
    "PacksStage",
    "Banner_02"
}
for _, folderName in pairs(foldersToDelete) do
    local folder = buildFolder:FindFirstChild(folderName)
    if folder then
        folder:Destroy()
    end
end
local objectsToRemove = {
    "Accessory",
    "Ball Theme",
    "ItemStand",
    "Taunt"
}
for _, objName in pairs(objectsToRemove) do
    for _, obj in pairs(miscFolder:GetChildren()) do
        if obj.Name == objName then
            obj:Destroy()
        end
    end
end
local museumFolder = lobby:FindFirstChild("Museum")
if museumFolder then
    local topText = museumFolder:FindFirstChild("TopText")
    if topText then
        topText:Destroy()
    end
end
local guiToRemove = {
    gameGui.LobbyHUD.BottomNavigationBar,
    gameGui.Store,
    gameGui.LobbyHUD.Topbar.Leaderstats.Coins,
    gameGui.Party
}
for _, guiElement in pairs(guiToRemove) do
    if guiElement then
        guiElement:Destroy()
    end
end
local workspaceToClear = {
    Workspace:FindFirstChild("Stadium") and Workspace.Stadium.Banners,
    Workspace:FindFirstChild("Environment"),
    Workspace:FindFirstChild("Stadium") and Workspace.Stadium.Flashes,
    Workspace:FindFirstChild("Stadium") and Workspace.Stadium.Build,
    workspace.Stadium.Teams.Home.Audience,
    workspace.Stadium.Teams.Away.Audience,
    workspace.Stadium.Sponsors
}
for _, obj in pairs(workspaceToClear) do
    if obj then
        obj:Destroy()
    end
end
local energyBars = gameGui:WaitForChild("MatchHUD"):WaitForChild("EnergyBars")
local function deleteStamina()
    for _, bar in pairs(energyBars:GetChildren()) do
        if bar.Name:sub(1,7) == "Stamina" then
            bar:Destroy()
        end
    end
end
energyBars.ChildAdded:Connect(function(child)
    if child.Name:sub(1,7) == "Stamina" then
        child:Destroy()
    end
end)
for _, bar in pairs(energyBars:GetChildren()) do
    if bar.Name == "Power" then
        bar.Size = UDim2.new(0.25, 0, 0.02, 0)
        local gradient = bar:WaitForChild("ProgressBar"):WaitForChild("UIGradient")
        gradient.Color = ColorSequence.new{
            ColorSequenceKeypoint.new(0, Color3.fromRGB(0,0,0)),
            ColorSequenceKeypoint.new(1, Color3.fromRGB(255,165,0))
        }
    end
end
local function removeDuplicates(name)
    local found = false
    for _, v in ipairs(gameGui:GetChildren()) do
        if v.Name == name then
            if not found then
                found = true
            else
                v:Destroy()
            end
        end
    end
end
local function removeGUIs()
    for _,g in pairs({
        gui:FindFirstChild("StarterPackInterface"),
        gameGui:FindFirstChild("Transition"),
        gameGui:FindFirstChild("KeybindDisplays"),
        gameGui:FindFirstChild("DailyRewards"),
        gameGui:FindFirstChild("GoalPopup")
    }) do
        if g then g:Destroy() end
    end
end
local function removeBlur()
    for _, effect in ipairs(Lighting:GetChildren()) do
        if effect:IsA("BlurEffect") then
            effect:Destroy()
        end
    end
end
local function removeArrows()
    for _, obj in ipairs(Workspace:GetChildren()) do
        if obj.Name == "Arrow" then
            obj:Destroy()
        end
    end
end
while true do
    deleteStamina()
    removeDuplicates("Scoreboard")
    removeDuplicates("PointsPopup")
    removeDuplicates("Summary")
    removeDuplicates("Penalties")
    removeGUIs()
    removeBlur()
    removeArrows()
    local scoreboard = gameGui:FindFirstChild("Scoreboard")
    if scoreboard then
        scoreboard.Size = UDim2.new(0.17, 0, 0.1, 0)
    end
    task.wait(5)
end
